{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "no-param-reassign": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-case-declarations": "off",
        "object-curly-newline": ["error", { "consistent": true }]
    }
}